{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.py": {
      "runtime": "python3.9",
      "config": {
        "includeFiles": [
          "lib2.py",
          "proto/**",
          "requirements.txt"
        ]
      }
    }
  }
}